{
  "name": "@veloze/session",
  "version": "0.2.2-0",
  "description": "session store",
  "keywords": [],
  "homepage": "https://github.com/commenthol/veloze-session",
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/veloze-session.git"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#*": {
      "default": "./src/*"
    }
  },
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "files": [
    "src",
    "types"
  ],
  "scripts": {
    "all": "npm-run-all lint c8 types",
    "changelog": "conv-changelog -o",
    "ci": "CI=true npm-run-all lint c8 types",
    "c8": "c8 -r lcov -r text npm t",
    "docs": "markedpp -s -i README.md",
    "dc": "docker compose --env-file .env -f ./scripts/docker-compose.yaml",
    "dc:down": "npm run dc -- down",
    "dc:up": "npm run dc -- up -d",
    "lint": "eslint .",
    "test": "mocha",
    "types": "rm -rf types; tsc && cp -r src/*.d.ts types/"
  },
  "mocha": {
    "color": true,
    "recursive": true,
    "timeout": 2000
  },
  "dependencies": {
    "debug-level": "^3.2.1",
    "veloze": "^1.0.0"
  },
  "devDependencies": {
    "@commenthol/dotconfig": "^1.5.0",
    "@types/node": "^22.9.0",
    "c8": "^10.1.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "ioredis": "^5.4.1",
    "mocha": "^10.8.2",
    "mongodb": "^6.10.0",
    "npm-run-all2": "^7.0.1",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
